<{% if section.settings.sticky_header_type != 'none' %}sticky-header data-sticky-type="{{ section.settings.sticky_header_type }}"{% else %}div{% endif %} class="header-wrapper">
  <header class="bt-header">
  
    <div class="bt-header-head-wrap">
      <div class="bt-container">
        <div class="bt-row align-items-center  justify-content-between bt-header-space">
          <div class="bt-col header-small-block">
            <a href="{{ routes.root_url }}" class="header-logo">
              {%- if settings.logo != blank -%}
                {%- assign logo_alt = settings.logo.alt | default: shop.name | escape -%}
                {%- assign logo_height = settings.logo_width | divided_by: settings.logo.aspect_ratio -%}
                {{ settings.logo | image_url: width: 500 | image_tag:
                  class: 'header__heading-logo',
                  widths: '50, 100, 150, 200, 250, 300, 400, 500',
                  height: logo_height,
                  width: settings.logo_width,
                  alt: logo_alt
