<style>
.menu-drawer-main-foot {
  display: flex;
  padding: 0 30px;
  flex-direction: column;
  align-items: center;
}
  .menu-drawer-main-foot > div {
    justify-items: center;
    align-items: center;
    display: grid;
    grid-template-columns: auto 1fr;
    gap: 11px;
  }
  .menu-drawer-main-foot a {
    padding: 3px 20px;
    background: #fff;
    color: #0000FF;
    border-radius: 10px;
    margin-top: 9px;
    font-family: 'Helvetica-Neue';
    font-weight: 400;
    font-size: 9px;
  }
  .menu-drawer-main-foot > div > div P {
    color: #fff;
    font-size: 11px;
    letter-spacing: -0.224px;
    line-height: 1;
    margin: 0 0 5px;
  }
.menu-drawer-main {
    display: flex;
    width: 100%;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
  }
  .menu-drawer-main .bt-col.header-small-block {
    max-width: 32%;
  }
  
  .menu-drawer-main .cart-drawer-support {
    display: flex;
    align-items: center;
    gap: 0;
    width: auto;
    justify-content: center;
    padding: 0;
    padding-left: 7px;
  }
  .menu-drawer-main .head-form-mob {
    margin: 30px 0 10px;
    width: 69%;
  }
  .menu-drawer-main .cart-drawer-support P {
    font-size: 13px;
    font-weight: 700;
    margin: 0;
    color: #fff;
    letter-spacing: -0.22px;
    font-family: 'Helvetica-Neue';
  }
  .menu-drawer-main .cart-drawer-support a {
    font-size: 11px;
    font-weight: 400;
    color: #fff;
    font-family: 'Helvetica-Neue';
    letter-spacing: -0.22px;
  }
  .head-form-pop form {
    min-width: 300px;
  }
  .head-form-pop {
    background: #0000D8;
    position: absolute;
    top: 154px;
    width: 100%;
    height: 300px;
    display: flex;
    align-items: center;
    justify-content: center;
    transform: translateY(-100%);
    z-index: -1;
    transition: all var(--duration-default) ease;
    opacity: 0;
    visibility: hidden;
}
  .head-form-pop .head-form-popover {
    content: '';
    position: absolute;
    width: 100%;
    height: calc(100vh + 100%);
}
.head-form-pop.sopen {
    transform: translateY(0);
    z-index: 0;
    visibility: visible;
    opacity: 1;
}
  .head-form-pop .field {
    background: #FFFFFF;
    border-radius: 10px;
    overflow: hidden;
  }
  .head-form-pop .field:after, .head-form-pop .field:before {
  box-shadow: none;
  border: none;
  }
  .head-form-pop input#Search-In-Modal {
    height: 61px;
    font-size: 14px;
    padding: 0 60px 0 20px;
    width: 100%;
    outline: none;
    box-shadow: none;
    color: #000;
    font-family: Inter;
    border: 0;
    outline: none;
}
  .head-form-pop button.search__button.field__button {
    max-width: 54px;
    width: 100%;
    height: 100%;
    background-color: #fff!important;
    border-radius: 0 10px 10px 0;
    top: 0;
    right: 0px;
    z-index: 1;
}
  header-drawer {
    justify-self: start;
  }

    .head-form-desk {
      display: none;
    }
    .head-form-mob {
      display: block;
    }
    .bt-row .bt-col {
      position: unset;
    }
  @media screen and (min-width: 992px) {
    header-drawer {
      display: none;
    }
    .head-form-desk {
      display: block;
    }
    .head-form-mob {
      display: none;
    }
  
    /* .list-menu--inline > :last-child a {
      padding: 5px 10px 2px;
      background: #fff;
      color: #0000D8;
      border-radius: 5px;
    } */
  .head-form-pop form {
    min-width: 700px;
  }
  }

  .menu-drawer-container {
    display: flex;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
  align-items: center;
  }

  .list-menu__item {
    display: flex;
    padding: 0;
    align-items: center;
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }
</style>

{%- style -%}
  .header__heading-logo {
    max-width: 94px;
  }
  
  {%- if section.settings.sticky_header_type == 'reduce-logo-size' -%}
    .scrolled-past-header .header__heading-logo {
      width: 75%;
    }
  {%- endif -%}
  
  .header {
    padding-top: {{ section.settings.padding_top | times: 0.5 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.5 | round: 0 }}px;
  }

  .section-header {
    position: sticky; /* This is for fixing a Safari z-index issue. PR #2147 */
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }

  @media screen and (min-width: 990px) {
    .header {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

