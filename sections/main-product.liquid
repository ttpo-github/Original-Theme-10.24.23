        <li><span data-id="#customtabing{{ forloop.index }}">{{ tabbing.tab_header }}</span></li>
        {% endfor %}
      </ul>
      <div id="descriptions" class="product-tabing-body">
        <div class="product__description rte quick-add-hidden">
